#include "solver.h"

int	insertion_sort(t_stack *s, t_list **l, t_stack_instructions si)
{
	int	y;
	int	i;
	int	cursor;

	i = 0;
	cursor = 0;

	while (cursor < s->size)
	{
		y = 0;
		while (y < cursor)
		{
			if (s->data[s->size - 1] > s->data[s->size - 2])
			{
				i += si.s(a, b, l);
				i += si.rr(a, b, l);
			}
			else
				break ;
			y += 1;
		}
		i += si.r(a, b, l);
		cursor += 1;

	}
	return (i);
}
